{
  "name": "@ianwremmel/tracks-controller",
  "version": "0.0.0-development",
  "description": "",
  "bin": {},
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ianwremmel/tracks-controller.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "build:es": "babel --source-maps --ignore '**/*.d.ts' --extensions '.js,.ts' -d dist src",
    "build:types": "tsc --emitDeclarationOnly",
    "eslint": "eslint --ignore --ignore-path .gitignore --ext .js --ext .ts",
    "lint": "npm-run-all lint:*",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "lint:js": "npm run --silent eslint -- .",
    "lint:types": "tsc --noEmit",
    "semantic-release": "semantic-release",
    "test": "jest"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@ianwremmel/typed-http-exceptions": "^1.1.1",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@ianwremmel/eslint-plugin-ianwremmel": "^3.0.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/node": "^12.0.4",
    "@types/supertest": "^2.0.7",
    "@typescript-eslint/parser": "^1.6.0",
    "condition-circle": "^2.0.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "jest-junit": "^6.3.0",
    "lint-staged": "^8.1.5",
    "markdown-toc": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "semantic-release": "^15.13.3",
    "supertest": "^4.0.2",
    "typescript": "^3.4.2"
  },
  "lint-staged": {
    "*.js": "npm run eslint -- ",
    "*.ts": "npm run eslint -- ",
    "bin/**": "npm run eslint -- "
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "publishConfig": {
    "access": "public"
  }
}
